package com.tntdjs.midi.controllers.novation.launchpad;

import com.tntdjs.midi.controllers.data.config.MidiDeviceXMLHelper;

/**
 * 
 * @author tsenauskas
 *
 */
public class LaunchpadSXMLHelper extends MidiDeviceXMLHelper {
	static LaunchpadSXMLHelper INSTANCE;
	
	/**
	 * 
	 * @return
	 */
	public static LaunchpadSXMLHelper getInstance() {
		if (null == INSTANCE) {
			INSTANCE = new LaunchpadSXMLHelper();
		}
		return INSTANCE;
	}

	/**
	 * 
	 */
	public String getXMLFileName() {
		return "config/midi/controllers/novation/launchpad/LaunchpadSConfig.xml";
	}
	
}
