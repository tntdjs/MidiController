public class MIDIFighterXMLHelper extends MidiDeviceXMLHelper {
	static MIDIFighterXMLHelper INSTANCE;
	
	/**
	 * 
	 * @return
	 */
	public static MIDIFighterXMLHelper getInstance() {
		if (null == INSTANCE) {
			INSTANCE = new MIDIFighterXMLHelper();
		}
		return INSTANCE;
	}

	/**
	 * 
	 */
	public String getXMLFileName() {
		return "config/midi/controllers/djtt/midifighter/MIDIFighterConfig.xml";
	}
	
}
