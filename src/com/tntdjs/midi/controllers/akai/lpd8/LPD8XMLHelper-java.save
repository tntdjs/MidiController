package com.tntdjs.midi.controllers.akai.lpd8;

import com.tntdjs.midi.controllers.data.config.MidiDeviceXMLHelper;

/**
 * 
 * @author tsenauskas
 *
 */
public class LPD8XMLHelper extends MidiDeviceXMLHelper {
	static LPD8XMLHelper INSTANCE;
	
	/**
	 * 
	 * @return
	 */
	public static LPD8XMLHelper getInstance() {
		if (null == INSTANCE) {
			INSTANCE = new LPD8XMLHelper();
		}
		return INSTANCE;
	}

	/**
	 * 
	 */
	public String getXMLFileName() {
		return "config/midi/controllers/akai/lpd8/LPD8Config.xml";
	}
	
}
